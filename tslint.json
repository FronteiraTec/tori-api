{
  "extends": "tslint:recommended",
  "rules": {
    "member-ordering": false,
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "quotemark": [true, "double", "avoid-escape", "avoid-template"],
    "semicolon": [true, "always"],
    "no-console": {
      "options": [true, "log"],
      "severity": "warning"
    }
  },
  "linterOptions": {
    "exclude": [
      "./node_modules/**"
    ]
  }
}